{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "array",
  "title": "The Root Schema",
  "items": {
    "$id": "#/items",
    "type": "object",
    "title": "The Items Schema",
    "required": [
      "product_name",
      "product_id",
      "type",
      "metric",
      "brand",
      "price"
    ],
    "properties": {
      "product_name": {
        "$id": "#/items/properties/product_name",
        "type": "string",
        "title": "The Product_name Schema",
        "default": "",
        "examples": [
          "carrot"
        ],
        "pattern": "^(.*)$"
      },
      "product_id": {
        "$id": "#/items/properties/product_id",
        "type": "string",
        "title": "The Product_id Schema",
        "default": "",
        "examples": [
          "V_34748"
        ],
        "pattern": "^(.*)$"
      },
      "type": {
        "$id": "#/items/properties/type",
        "type": "string",
        "title": "The Type Schema",
        "default": "",
        "examples": [
          "vegetable"
        ],
        "pattern": "^(.*)$"
      },
      "metric": {
        "$id": "#/items/properties/metric",
        "type": "string",
        "title": "The Metric Schema",
        "default": "",
        "examples": [
          "kg"
        ],
        "pattern": "^(.*)$"
      },
      "brand": {
        "$id": "#/items/properties/brand",
        "type": "string",
        "title": "The Brand Schema",
        "default": "",
        "examples": [
          "NA"
        ],
        "pattern": "^(.*)$"
      },
      "price": {
        "$id": "#/items/properties/price",
        "type": "integer",
        "title": "The Price Schema",
        "default": 0,
        "examples": [
          30
        ]
      }
    }
  }
}